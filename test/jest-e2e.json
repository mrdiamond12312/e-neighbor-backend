{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "moduleDirectories": ["<rootDir>/../", "node_modules"],
  "moduleNameMapper": {
    "@/(.*)$": "<rootDir>/../src/$1"
  },
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
